{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {
            "source": "/((?!sitemap\\.xml|robots\\.txt|google.*\\.html|.*\\.(ico|png|jpg|jpeg|svg|css|js|json)).*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/cleanup-sessions",
            "schedule": "0 19 * * *"
        }
    ]
}